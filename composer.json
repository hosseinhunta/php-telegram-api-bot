{
    "name": "hosseinhunta/php-telegram-api-bot",
    "description": "A PHP SDK for Telegram Bot API with full method support and robust error handling",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hosseinhunta\\PhpTelegramBotApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hossein Mohammadian",
            "email": "hosseinhunta@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "config": {
        "http-basic": {},
        "proxy": {
            "http": "http://127.0.0.1:10808",
            "https": "http://127.0.0.1:10808"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "monolog/monolog": "^3.9",
        "ext-curl": "*"
    }
}
